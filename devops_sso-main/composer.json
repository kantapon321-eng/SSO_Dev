{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "appzcoder/laravel-admin": "^2.1",
        "arcanedev/log-viewer": "^4.5",
        "bacon/bacon-qr-code": "^2.0",
        "fideloper/proxy": "^4.0",
        "kyslik/column-sortable": "^5.8",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "paragonie/constant_time_encoding": "^2.5",
        "pragmarx/google2fa-laravel": "^2.0",
        "spatie/laravel-activitylog": "^2.5",
        "spatie/laravel-permission": "^2.12",
        "yajra/laravel-datatables-buttons": "^4.6",
        "yajra/laravel-datatables-html": "^4.0",
        "yajra/laravel-datatables-oracle": "8.x"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Helpers/Helper.php",
            "app/Helpers/HelperAPI.php"
        ]

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}